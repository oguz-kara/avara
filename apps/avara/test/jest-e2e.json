{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../libs",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@avara/core/(.*)$": "<rootDir>/@core/src/$1",
    "^@avara/shared/(.*)$": "<rootDir>/@shared/src/$1",
    "^src/(.*)$": "<rootDir>/../src/$1"
  }
}
