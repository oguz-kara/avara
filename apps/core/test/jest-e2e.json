{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@avara/core(|/.*)$": "<rootDir>/../../core/src/$1",
    "^@avara/shared(|/.*)$": "<rootDir>/../../../libs/@shared/src/$1"
  }
}
