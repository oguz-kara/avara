{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/avara/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/avara/tsconfig.app.json"
  },
  "projects": {
    "@core": {
      "type": "library",
      "root": "libs/@core",
      "entryFile": "index",
      "sourceRoot": "libs/@core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/@core/tsconfig.lib.json"
      }
    },
    "@shared": {
      "type": "library",
      "root": "libs/@shared",
      "entryFile": "index",
      "sourceRoot": "libs/@shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/@shared/tsconfig.lib.json"
      }
    },
    "avara": {
      "type": "application",
      "root": "apps/avara",
      "entryFile": "main",
      "sourceRoot": "apps/avara/src",
      "compilerOptions": {
        "tsConfigPath": "apps/avara/tsconfig.app.json"
      }
    },
    "uilocalize": {
      "type": "application",
      "root": "apps/uilocalize",
      "entryFile": "main",
      "sourceRoot": "apps/uilocalize/src",
      "compilerOptions": {
        "tsConfigPath": "apps/uilocalize/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/avara"
}